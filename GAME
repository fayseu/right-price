g = "y"
while g.lower() in {"y"} :
    import random
    a=random.randint(1,1001)
    p=int(input("Donne un nombre: "))
    while a!=p :
        if a>p :
            print("+")
        elif a<p : 
            print("-")
        else :
            print("VICTOIRE!")
            break
        p=int(input("Donne un nombre: "))
    g=str(input("Restart ? (y/n) :  "))
    if g=="n" :
        break
